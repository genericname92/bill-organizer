<% if (window.CURRENT_USER_ID === parseInt(bill.escape("owner_id"))){ %>
<button
  class="deleteBill close" title="Delete this bill!">&times;
</button>
<% } %>
<div class="billInfo">
<a href="#bills/<%=bill.id%>"><%= bill.escape("title") %></a><br>
<div class="date">From <%= from_date %> to <%= end_date %></div>
<div class="date">Last Updated: <%= date %></div>
</div>
